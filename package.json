{
  "name": "novo-portal-assertiva",
  "version": "1.0.0",
  "description": "Dashboard criado com React + Redux + Superagent + Webpack",
  "main": "index.js",
  "repository": "https://github.com/henriquejensen/dashboard.git",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "build": "webpack -p && npm run build:run",
    "build:run": "node server.js",
    "deploy": "aws s3 sync public s3://front.assertiva && npm run deploy:style && npm run deploy:invalidation",
    "deploy:style": "aws s3 sync style s3://front.assertiva/style",
    "deploy:invalidation": "aws cloudfront create-invalidation --distribution-id EGO3V7A2E1X5N --paths /*",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:update": "jest -u"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.2.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.28.2",
    "enzyme": "^2.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "node-sass": "^4.1.1",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^2.7.4",
    "sass-loader": "^4.1.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "chart.js": "^2.4.0",
    "express": "^4.14.0",
    "file-saver": "^1.3.3",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.7",
    "react-breadcrumbs": "^1.5.1",
    "react-chartjs-2": "^1.6.2",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.3.0",
    "react-gmaps": "^1.5.0",
    "react-notification-system": "^0.2.11",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-tooltip": "^3.2.2",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.18.1",
    "superagent": "^3.3.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
